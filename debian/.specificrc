#!/usr/bin/env bash

alias acs='apt-cache search'
alias apta='sudo apt-add-repository'
alias aptup='sudo apt update && sudo apt upgrade && sudo apt autoremove'
alias apti='sudo apt install --no-install-recommends'
alias aptr='sudo apt remove'
alias aptsrc='cat /etc/apt/sources.list /etc/apt/sources.list.d/*'

alias snvm='source "${HOME}/.nvm/nvm.sh"'
